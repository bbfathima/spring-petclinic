  pipeline {
  agent { label 'FATHIMA'}
    stages {
        stage('git clone') {
          steps {
            git url: 'https://github.com/bbfathima/spring-petclinic.git',
               branch: 'declarative'
          }
        }
        stage('build') {
          steps {
            sh './mvnw package'
          }
        }
    }
  }
